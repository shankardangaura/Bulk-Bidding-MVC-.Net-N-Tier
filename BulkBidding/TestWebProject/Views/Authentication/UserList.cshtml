@model IEnumerable<BRDataModels.Authentication>
<h1>User List</h1>

<table>
    <tr>
        <td>S.N</td>
        <td>Username</td>
        <td>Email</td>
        <td>Action</td>
    </tr>
    @foreach(var user in Model)
    {
        <tr>
            <td></td>
            <td>@user.userName</td>
            <td>@user.Email</td>
            <td>
                <a href="#">Edit</a>
                <a href="#">Delete</a>
            </td>
        </tr>
    }
</table>